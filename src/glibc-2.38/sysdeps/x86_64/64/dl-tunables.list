glibc {
  cpu {
    prefer_map_32bit_exec {
      type: INT_32
      minval: 0
      maxval: 1
      env_alias: LD_PREFER_MAP_32BIT_EXEC
      security_level: SXID_IGNORE
    }
  }
}

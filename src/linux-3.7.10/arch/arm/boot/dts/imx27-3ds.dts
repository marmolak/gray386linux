/dts-v1/;
/include/ "imx27.dtsi"

/ {
	model = "mx27_3ds";
	compatible = "freescale,imx27-3ds", "fsl,imx27";

	memory {
		reg = <0x0 0x0>;
	};

	soc {
		aipi@10000000 { /* aipi */

			wdog@10002000 {
				status = "okay";
			};

			uart1: serial@1000a000 {
				fsl,uart-has-rtscts;
				status = "okay";
			};

			fec@1002b000 {
				status = "okay";
			};
		};
	};

};
